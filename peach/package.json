{
  "name": "peach",
  "version": "0.0.1",
  "description": "Peach Application",
  "author": "Peach2Peach",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "start:web": "cp -r ./assets ./public/assets/ && react-scripts start",
    "build:web": "cp -r ./assets ./public/assets/ && react-scripts build",
    "style:update": "create-tailwind-rn && mv ./styles.json ./src/styles/styles.json",
    "test": "jest",
    "lint": "eslint ."
  },
  "dependencies": {
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-native": "0.64.2",
    "react-native-web": "0.17.0",
    "react-scripts": "3.4.1",
    "tailwind-react-native-classnames": "^1.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "babel-jest": "^26.6.3",
    "eslint": "7.14.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.64.0",
    "react-test-renderer": "17.0.1",
    "tailwind-rn": "^3.0.1",
    "tailwindcss": "^2.2.7"
  },
  "jest": {
    "preset": "react-native"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
