{
  "name": "peach",
  "version": "0.4.2",
  "description": "Peach Application",
  "author": "Peach2Peach",
  "private": true,
  "scripts": {
    "web": "cp -r ./assets ./public/assets/ && react-scripts start",
    "android": "NODE_ENV=sandbox react-native run-android --port=8081 --variant=QaDebug",
    "android:testnet": "NODE_ENV=development react-native run-android --port=8081 --variant=QaDebug",
    "android:mainnet": "NODE_ENV=production react-native run-android --port=8081 --variant=ProdDebug --appIdSuffix=mainnet",
    "ios": "NODE_ENV=sandbox react-native run-ios --port=8081 --simulator=\"iPhone 14\" --scheme \"peach testnet\"",
    "ios:testnet": "NODE_ENV=development react-native run-ios --port=8081 --simulator=\"iPhone 14\" --variant=QaDebug",
    "ios:mainnet": "NODE_ENV=production react-native run-ios --port=8081 --simulator=\"iPhone 14\" --variant=ProdDebug --appIdSuffix=mainnet",
    "start": "NODE_ENV=sandbox react-native start --port=8081",
    "start:testnet": "NODE_ENV=development react-native start --port=8081",
    "start:mainnet": "NODE_ENV=production react-native start --port=8081",
    "cache:clear": "react-native start --reset-cache",
    "postinstall": "rn-nodeify --install buffer,stream,util --hack",
    "podinstall": "react-native setup-ios-permissions && cd ios && pod install --repo-update",
    "build:web": "cp -r ./assets ./public/assets/ && react-scripts build",
    "build:android:testnet": "./devUtils/buildAndroidTestnet.sh",
    "build:android:mainnet": "./devUtils/buildAndroidMainnet.sh",
    "bundle:android:testnet": "./devUtils/bundleAndroidTestnet.sh",
    "bundle:android:mainnet": "./devUtils/bundleAndroidMainnet.sh",
    "deploy:android:testnet": "./devUtils/buildAndroidTestnet.sh && ./devUtils/bundleAndroidTestnet.sh",
    "deploy:android:mainnet": "./devUtils/buildAndroidMainnet.sh && ./devUtils/bundleAndroidMainnet.sh",
    "prepare:release": "./devUtils/prepareRelease.sh",
    "build:ios:testnet": "./devUtils/buildIOSTestnet.sh",
    "build:ios:mainnet": "./devUtils/buildIOSMainnet.sh",
    "bump:version": "react-native-version --never-amend",
    "postversion": "react-native-version",
    "test": "jest ./src",
    "test:coverage": "jest --coverage ./src",
    "test:ci": "jest --maxWorkers=100% --coverage --ci --forceExit ./src",
    "test:e2e:android:build": "detox build --configuration android",
    "test:e2e:android": "detox test --configuration android --config ./tests/e2e/jest.config.js --loglevel trace --record-logs all",
    "test:e2e:ios:build": "detox build --configuration ios",
    "test:e2e:ios": "detox test --configuration ios --config ./tests/e2e/jest.config.js",
    "test:detox:clean": "detox clean-framework-cache && detox build-framework-cache",
    "tsc": "tsc",
    "lint": "eslint . --ext .tsx,.ts"
  },
  "dependencies": {
    "@bitcoinerlab/secp256k1": "^1.0.5",
    "@msml/react-native-notification-badge": "^0.1.6",
    "@react-native-clipboard/clipboard": "^1.11.2",
    "@react-native-community/cli-platform-android": "^11.3.6",
    "@react-native-community/netinfo": "^9.4.1",
    "@react-native-cookies/cookies": "^6.2.1",
    "@react-native-firebase/analytics": "^18.2.0",
    "@react-native-firebase/app": "^18.2.0",
    "@react-native-firebase/crashlytics": "^18.2.0",
    "@react-native-firebase/messaging": "^18.2.0",
    "@react-native-firebase/perf": "^18.2.0",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/material-top-tabs": "^6.6.3",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/stack": "^6.3.16",
    "@tanstack/react-query": "^5.17.19",
    "assert": "^2.0.0",
    "bdk-rn": "github:Peach2Peach/bdk-rn",
    "bip174": "^2.1.1",
    "bip32": "^4.0.0",
    "bip322-js": "github:Peach2Peach/bip322-js#bugfix/compilation-on-npm-install",
    "bip39": "^3.1.0",
    "bitcoinjs-lib": "^6.1.5",
    "bitcoinjs-message": "^2.2.0",
    "buffer": "^6.0.3",
    "ecpair": "^2.1.0",
    "events": "^3.3.0",
    "iban": "^0.0.14",
    "jotai": "^2.6.4",
    "lottie-react-native": "^6.0.1",
    "node-forge": "file:./__mocks__/node-forge",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "react": "18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.2",
    "react-native": "^0.71.14",
    "react-native-camera": "^4.2.1",
    "react-native-crypto-js": "^1.0.0",
    "react-native-device-info": "^10.6.0",
    "react-native-document-picker": "^9.0.1",
    "react-native-dotenv": "^3.4.8",
    "react-native-fast-openpgp": "2.6.0",
    "react-native-fs": "^2.20.0",
    "react-native-gesture-handler": "^2.13.4",
    "react-native-level-fs": "^3.0.1",
    "react-native-mmkv-storage": "^0.9.1",
    "react-native-pager-view": "^6.2.0",
    "react-native-parsed-text": "^0.0.22",
    "react-native-permissions": "^3.8.4",
    "react-native-promise-rejection-utils": "^0.0.1",
    "react-native-qrcode-svg": "^6.2.0",
    "react-native-reanimated": "^3.3.0",
    "react-native-reanimated-carousel": "^3.5.1",
    "react-native-restart": "^0.0.27",
    "react-native-safe-area-context": "^4.7.1",
    "react-native-screens": "^3.20.0",
    "react-native-securerandom": "^1.0.1",
    "react-native-share": "^8.2.2",
    "react-native-splash-screen": "^3.3.0",
    "react-native-svg": "^13.9.0",
    "react-native-tab-view": "^3.5.2",
    "react-native-url-polyfill": "^1.3.0",
    "react-native-webview": "^13.6.2",
    "readable-stream": "^4.3.0",
    "rn-nodeify": "^10.3.0",
    "stream-browserify": "^3.0.0",
    "twrnc": "^3.6.7",
    "util": "^0.12.5",
    "varuint-bitcoin": "^1.1.2",
    "zod": "^3.21.4",
    "zustand": "^4.3.6"
  },
  "react-native": {
    "path": "path-browserify",
    "fs": "react-native-level-fs",
    "_stream_transform": "readable-stream/transform",
    "_stream_readable": "readable-stream/readable",
    "_stream_writable": "readable-stream/writable",
    "_stream_duplex": "readable-stream/duplex",
    "_stream_passthrough": "readable-stream/passthrough",
    "stream": "stream-browserify"
  },
  "devDependencies": {
    "@babel/core": "^7.21.3",
    "@babel/preset-env": "^7.20.2",
    "@babel/runtime": "^7.21.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@testing-library/react-native": "^12.0.1",
    "@tsconfig/react-native": "^2.0.2",
    "@types/iban": "^0.0.32",
    "@types/jest": "^29.5.0",
    "@types/react": "^18.0.30",
    "@types/react-native": "^0.71.7",
    "@types/react-native-dotenv": "^0.2.0",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "babel-jest": "^29.5.0",
    "detox": "^20.6.0",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-react-hooks": "^4.4.0",
    "eslint-plugin-react-native": "^4.0.0",
    "jest": "^29.5.0",
    "jest-transform-stub": "^2.0.0",
    "metro-react-native-babel-preset": "^0.76.0",
    "prettier": "3.2.4",
    "react-native-svg-transformer": "^1.0.0",
    "react-native-version": "^4.0.0",
    "react-test-renderer": "18.2.0",
    "snapshot-diff": "^0.10.0",
    "typescript": "^5.1.6"
  },
  "rnpm": {
    "assets": [
      "./assets/fonts/"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "browser": {
    "path": "path-browserify",
    "fs": "react-native-level-fs",
    "_stream_transform": "readable-stream/transform",
    "_stream_readable": "readable-stream/readable",
    "_stream_writable": "readable-stream/writable",
    "_stream_duplex": "readable-stream/duplex",
    "_stream_passthrough": "readable-stream/passthrough",
    "stream": "stream-browserify"
  },
  "reactNativePermissionsIOS": [
    "Camera",
    "Notifications"
  ]
}
